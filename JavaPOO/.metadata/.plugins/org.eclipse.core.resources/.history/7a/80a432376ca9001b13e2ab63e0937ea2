
public class Node
{
	int value;
	static Node previous;
	static Node next;
	
//	Node(){
//		previous = null;
//		next =null;
//	}
	
	Node(int value)
	{
		this.value = value;
		previous = null;
		next = null;
	}
	
	public void getNext(Node next) //get next node
	{
		this.next = next; 
	}
	
	public void getPrevious(Node previous) //get previous node
	{
		this.previous = previous; 
	}
	
}
