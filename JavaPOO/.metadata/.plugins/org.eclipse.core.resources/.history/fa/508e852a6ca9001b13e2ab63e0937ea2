
public class List 
{
	static Node head ;
	static Node tail;
	int count; //length of the list
	
	public List() { //empty List
		head = null;
		tail = null;
		count = 0;
	}
	
	//method to know if the list empty is or not
	static boolean emptyList(List list)
	{
		if(list.head == null && list.tail == null)
			return true;
		return false;
	}
	
	public static List insert(Node x, int p)
	{
		List list = new List();
		
		if(p == 0) {
			head.next = x;
		}
		
		
		return list;
	}
	
	public static void main(String[] args) {
		
	}
	
}
