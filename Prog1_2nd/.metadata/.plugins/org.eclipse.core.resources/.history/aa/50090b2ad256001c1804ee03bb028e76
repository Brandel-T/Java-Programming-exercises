
public class CyclicalNumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(getLenght(1245345));
	}

	//Laenge einer Zahle berechnen
	public static long getLenght(long number) 
	{ 
		if ( number < 0)
			return -1;
		else {
			long n = 1;
			long top = 1;
			while (number > 10*top) {
				n++;
				top = 10*top;
			}
			
			return n;
		}
	}
	
	
}
